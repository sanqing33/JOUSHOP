{
	"extends": "@vue/tsconfig/tsconfig.json",
	"compilerOptions": {
		"sourceMap": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"lib": ["esnext", "dom"],
		"types": ["@dcloudio/types", "uview-plus/types"],
		"preserveValueImports": false,
		"isolatedModules": false
	},
	"include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}